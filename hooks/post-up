#!/bin/sh
echo 'Running rcm post-up hook'

if [ "$(uname)" = "Darwin" ]; then
  $DOT_PATH/hooks/bin/osx/postinstall
elif [ "$(expr substr $(uname -s) 1 5)" = "Linux" ]; then
  $DOT_PATH/hooks/bin/nix/postinstall
fi

# $DOT_PATH/hooks/bin/bash/postinstall
# NB: causes cli to freeze up
# $DOT_PATH/hooks/bin/vim/postinstall
